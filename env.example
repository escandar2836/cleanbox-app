# CleanBox environment variable settings example
# Copy this file to .env and edit with actual values

# Flask settings
FLASK_APP=run.py
FLASK_ENV=development
FLASK_PORT=5001

# Database settings (PostgreSQL)
POSTGRES_DB=cleanbox
POSTGRES_USER=cleanbox_user
POSTGRES_PASSWORD=cleanbox_password
CLEANBOX_DATABASE_URI=postgresql://cleanbox_user:cleanbox_password@postgres:5432/cleanbox

# Security key settings (change to actual values)
CLEANBOX_SECRET_KEY=your-secret-key-here
CLEANBOX_ENCRYPTION_KEY=ps3UBGLk54b5JTRvUDemiIVX0c84WTH5EE0UbgSuhAo=

# Google OAuth settings
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:5001/auth/callback

# AI settings (using Ollama)
CLEANBOX_USE_OLLAMA=true
OLLAMA_URL=http://ollama:11434
OLLAMA_MODEL=llama2

# OAuth development environment settings
OAUTHLIB_INSECURE_TRANSPORT=1
